{
  "name": "usestatemate",
  "displayName": "useStateMate",
  "description": "Simplify React state management with useState setter suggestions and auto-generation.",
  "version": "0.0.4",
  "publisher": "aviralj02",
  "icon": "./media/usestatemate-icon.png",
  "engines": {
    "vscode": "^1.95.0"
  },
  "author": {
    "name": "aviral"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "keywords": [
    "usestatemate",
    "helper",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "react",
    "js",
    "jsx",
    "ts",
    "tsx"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aviralj02/useStateMate.git"
  },
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.addUseStateWithTypes",
        "title": "Add useState with Type Inference"
      }
    ],
    "languages": [
      {
        "id": "typescript",
        "extensions": [
          ".ts"
        ],
        "aliases": [
          "TypeScript"
        ]
      },
      {
        "id": "javascript",
        "extensions": [
          ".js"
        ],
        "aliases": [
          "JavaScript"
        ]
      },
      {
        "id": "typescriptreact",
        "extensions": [
          ".tsx"
        ],
        "aliases": [
          "TypeScript React"
        ]
      },
      {
        "id": "javascriptreact",
        "extensions": [
          ".jsx"
        ],
        "aliases": [
          "JavaScript React"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
